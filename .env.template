# Data89 Evaluation Pipeline Configuration Template
# Copy this file to .env and fill in your actual credentials

# Qualtrics API Configuration
QUALTRICS_API_TOKEN=your_api_token_here
QUALTRICS_DATACENTER=your_datacenter_id_here
QUALTRICS_SURVEY_ID=your_survey_id_here

# Model Configuration
# Mastery threshold (probability below which students are flagged)
MASTERY_THRESHOLD=0.7

# Pipeline Configuration
# Number of chains for Stan sampling
N_CHAINS=4

# Number of sampling iterations
N_ITER=2000
